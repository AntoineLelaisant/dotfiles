! ------------------------------------------------------------------------------
! ROFI Color theme
! ------------------------------------------------------------------------------
rofi.color-enabled:               true
rofi.color-window:                argb:d92f343f, argb:0002b36, argb:0000000
rofi.color-normal:                argb:0f3f4f5, #f3f4f5, argb:0f3f4f5, argb:0f3f4f5, #9575cd
rofi.color-active:                argb:0f3f4f5, #9575cd, argb:0f3f4f5, argb:0f3f4f5, #f3f4f5
rofi.color-urgent:                argb:0f3f4f5, #f3f4f5, argb:0f3f4f5, argb:0dc322f, #f3f4f5

! Misc
xft.lcdfilter:                    ldcdefault
xft.hintstyle:                    hintslight

! Visual 
URxvt.geometry:                   90x25 
URxvt.title:                      Terminal
URxvt.depth:                      32
URxvt.borderless:                 1
URxvt.scrollBar:                  false
URxvt.cursorBlink:                1
URxvt.cursorUnderline:            1
URxvt.font:                       xft:DejaVuSansMono Nerd Font:size=12:antialias=true

! Plugins
URxvt.perl-ext-common:            default,matcher,tabbedex
URxvt.url-launcher:               /usr/bin/xdg-open

! Tabbedex color
URxvt.tabbed.tabbar-fg:           7
URxvt.tabbed.tabbar-bg:           0
URxvt.tabbed.tab-fg:              11
URxvt.tabbed.tab-bg:              0

! Tabs title
URxvt.tabbed.title:               true
URxvt.tabbed.title-bg:            0 
URxvt.tabbed.title-fg:            7
URxvt.urlLauncher:                firefox
URxvt.matcher.button:             1
URxvt.clipboard.autocopy:         true

! History
URxvt.saveLines:                  5000

URxvt*depth:                      32

! colors
URxvt*background:                 [80]#000000
URxvt*foreground:                 [100]#ffffff
URxvt.transparent:                true
URxvt.shading:                    0
URxvt.cursorColor:                #ebdbb2
